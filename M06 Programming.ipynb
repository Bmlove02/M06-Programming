    "import time\n",
    "from datetime import date\n",
    "#13.1\n",
    "n = date.today()\n",
    "fin = open(\"today.txt\",'w')\n",
    "fin.write('%s'%n)\n",
    "fin.close()\n",
    "\n",
    "#13.2\n",
    "fin = open(\"today.txt\", 'r')\n",
    "today_string = fin.readline()\n",
    "#13.3\n",
    "fmt = \"%Y-%m-%d\"\n",
    "time.strptime(today_string, fmt)\n",
    "print(today_string)\n",
    "fin.close()"
   ]
